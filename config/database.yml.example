default: &default
  adapter: mysql2
  encoding: utf8
  pool: 5
  username: username
  password: password
  socket: socket

development:
  <<: *default
  database: whatsthediff_dev

# Warning: The database defined as "test" will be erased and
# re-generated from your development database when you run "rake".
# Do not set this db to the same as development or production.
test:
  <<: *default
  database: whatsthediff_test

production:
  <<: *default
  database: whatsthediff_prod